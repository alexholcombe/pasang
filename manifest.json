{
    "name": "pasang",
    "description": "Add contributor information automatically to submission portals.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["scripting", "activeTab", "tabs", "storage"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
      }
  }